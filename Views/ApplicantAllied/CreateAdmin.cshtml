@model Bt.Models.ViewModel.AlliedViewModel;

@{
    ViewData["Title"]="Create";
    Layout = "_Layout";

}
<style>
    hr {
        border: 10px solid green;
        border-radius: 5px;
    }

    div {
        padding: 2px;
    }
</style>

<div class="offset-1">
    <h1>Create</h1>

    <h4>Applicants Allied</h4>
    <hr class="col-md-10" />
</div>

<div class="row offset-md-1">
    <div>
        <form asp-action="Create" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="AplicantName" class="control-label"></label>
                        <input asp-for="AplicantName" class="form-control" />
                        <span asp-validation-for="AplicantName" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Dob" class="control-label"></label>
                        <input type="date" asp-for="Dob" class="form-control" />
                        <span asp-validation-for="Dob" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Gender" class="control-label"></label>
                        <span asp-validation-for="Gender" class="text-danger"></span>
                        <select asp-for="Gender" class="form-control" id="Gender"></select>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Mobile" class="control-label"></label>
                        <input asp-for="Mobile" class="form-control" />
                        <span asp-validation-for="Mobile" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="EMail" class="control-label"></label>
                        <input asp-for="EMail" class="form-control" />
                        <span asp-validation-for="EMail" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Nationality" class="control-label"></label>
                        <span asp-validation-for="Nationality" class="text-danger"></span>
                        <select id="Nationality" asp-for="Nationality" class="form-control"></select>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Religion" class="control-label"></label>
                        <span asp-validation-for="Religion" class="text-danger"></span>
                        <select asp-for="Religion" class="form-control" id="Religion"></select>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Category" class="control-label"></label>
                        <span asp-validation-for="Category" class="text-danger"></span>
                        <select id="Category" asp-for="Category" class="form-control"></select>
                    </div>
                </div>
                <div class="col-md-5">
                </div>
            </div>
            <hr class="col-md-10" />
            <div class="row">
                <h3>Academic Information</h3>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <label asp-for="PercentageXii" class="control-label">Class 12 Percentage</label>
                    <input type="number" asp-for="PercentageXii" class="form-control" />
                    <span asp-validation-for="PercentageXii" class="text-danger"></span>
                </div>
                <div class="col-md-5">
                    <label asp-for="PercentagePcbXii" class="control-label">Class 12 PCB Percentage</label>
                    <input type="number" asp-for="PercentagePcbXii" class="form-control" />
                    <span asp-validation-for="PercentagePcbXii" class="text-danger"></span>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <label asp-for="PercentagePcmXii" class="control-label">Class 12 PCM Percentage</label>
                    <input type="number" asp-for="PercentagePcmXii" class="form-control" />
                    <span asp-validation-for="PercentagePcmXii" class="text-danger"></span>
                </div>

            </div>
            <div class="row">
                <div class="col-md-5">
                    <h4>Physics</h4>
                    <label asp-for="MarksPhysicsTheory" class="control-label">Physics Theory Marks</label>
                    <input type="number" asp-for="MarksPhysicsTheory" class="form-control" />
                    <span asp-validation-for="MarksPhysicsTheory" class="text-danger"></span>

                    <label asp-for="MarksPhysicsPractical" class="control-label">Physics Practical Marks</label>
                    <input type="number" asp-for="MarksPhysicsPractical" class="form-control" />
                    <span asp-validation-for="MarksPhysicsPractical" class="text-danger"></span>

                    <label asp-for="FullmarksPhysics" class="control-label">Physics Full Marks</label>
                    <input type="number" asp-for="FullmarksPhysics" class="form-control" />
                    <span asp-validation-for="FullmarksPhysics" class="text-danger"></span>
                </div>

                <div class="col-md-5">
                    <h4>Chemistry</h4>
                    <label asp-for="MarksChemistryTheory" class="control-label">Chemistry Theory Marks</label>
                    <input type="number" asp-for="MarksChemistryTheory" class="form-control" />
                    <span asp-validation-for="MarksChemistryTheory" class="text-danger"></span>

                    <label asp-for="MarksChemistryPractical" class="control-label">Chemistry Practical Marks</label>
                    <input type="number" asp-for="MarksChemistryPractical" class="form-control" />
                    <span asp-validation-for="MarksChemistryPractical" class="text-danger"></span>

                    <label asp-for="FullmarksChemistry" class="control-label">Chemistry Full Marks</label>
                    <input type="number" asp-for="FullmarksChemistry" class="form-control" />
                    <span asp-validation-for="FullmarksChemistry" class="text-danger"></span>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <h4>Biology</h4>
                        <label asp-for="MarksBiologyTheory" class="control-label">Biology Theory Marks</label>
                        <input type="number" asp-for="MarksBiologyTheory" class="form-control" />
                        <span asp-validation-for="MarksBiologyTheory" class="text-danger"></span>

                        <label asp-for="MarksBiologyPractical" class="control-label">Biology Practical Marks</label>
                        <input type="number" asp-for="MarksBiologyPractical" class="form-control" />
                        <span asp-validation-for="MarksBiologyPractical" class="text-danger"></span>

                        <label asp-for="FullmarksBiology" class="control-label">Biology Full Marks</label>
                        <input type="number" asp-for="FullmarksBiology" class="form-control" />
                        <span asp-validation-for="FullmarksBiology" class="text-danger"></span>
                    </div>
                </div>

                <div class="col-md-5">
                    <div class="form-group">
                        <h4>Mathematics</h4>
                        <label asp-for="MarksMathematicsTheory" class="control-label">Mathematics Theory Marks</label>
                        <input type="number" asp-for="MarksMathematicsTheory" class="form-control" />
                        <span asp-validation-for="MarksMathematicsTheory" class="text-danger"></span>

                        <label asp-for="MarksMathematicsPractical" class="control-label">Mathematics Pratical Marks</label>
                        <input type="number" asp-for="MarksMathematicsPractical" class="form-control" />
                        <span asp-validation-for="MarksMathematicsPractical" class="text-danger"></span>

                        <label asp-for="FullmarksMathematics" class="control-label">Mathematics Full Marks</label>
                        <input type="number" asp-for="FullmarksMathematics" class="form-control" />
                        <span asp-validation-for="FullmarksMathematics" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5">
                   
                </div>
                <div class="col-md-5">
                   
                </div>

            </div>
            <hr class="col-md-10" />
            <div class="row">
                <div class="col-md-5">
                    <div class="row">
                        <h3>Permanent Address</h3>
                    </div>
                    <div class="form-group">
                        <label asp-for="PermanentState" class="control-label"></label>
                        <select id="PerState" class="form-control" asp-for="PermanentState">
                        </select>
                    </div>
                    <div class="form-group">
                        <label asp-for="PermanentDistrict" class="control-label"></label>
                        <select id="PerDistrict" class="form-control" asp-for="PermanentDistrict">
                        </select>
                    </div>
                    <div class="form-group">
                        <label asp-for="PermanentBlock" class="control-label"></label>
                        <select id="PerBlock" class="form-control" asp-for="PermanentBlock">
                        </select>
                    </div>
                    <div class="form-group">
                        <label asp-for="PermanentVillage" class="control-label"></label>
                        <select id="PerVillage" class="form-control" asp-for="PermanentVillage">
                        </select>
                    </div>

                    <div class="form-group">
                        <label asp-for="PermanentAddress" class="control-label"></label>
                        <input asp-for="PermanentAddress" class="form-control" />
                        <span asp-validation-for="PermanentAddress" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="row">
                        <h3>Present Address</h3>
                    </div>
                    <div class="form-group">
                        <label asp-for="PresentState" class="control-label"></label>
                        <select id="PreState" class="form-control" asp-for="PresentState">
                        </select>
                        <span asp-validation-for="PresentState" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PresentDistrict" class="control-label"></label>
                        <select id="PreDistrict" class="form-control" asp-for="PresentDistrict">
                        </select>
                        <span asp-validation-for="PresentDistrict" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PresentBlock" class="control-label"></label>
                        <select id="PreBlock" class="form-control" asp-for="PresentBlock">
                        </select>
                        <span asp-validation-for="PresentBlock" class="text-danger"></span>
                    </div>

                    <div class="form-group">
                        <label asp-for="PresentVillage" class="control-label"></label>
                        <select id="PreVillage" class="form-control" asp-for="PresentVillage">
                        </select>
                        <span asp-validation-for="PresentVillage" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PresentAddress" class="control-label"></label>
                        <input asp-for="PresentAddress" class="form-control" />
                        <span asp-validation-for="PresentAddress" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-5">
                </div>
                <div class="col-md-5">
                </div>
            </div>
            <hr class="col-md-10" />
            <div class="row">
                <h3>Document Upload</h3>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="PassportPhoto" class="control-label"></label>
                        <input type="file" asp-for="PassportPhoto" class="form-control" name="passportPhoto" />
                        <span asp-validation-for="PassportPhoto" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="PrCetificate" class="control-label"></label>
                        <input type="file" asp-for="PrCetificate" class="form-control" />
                        <span asp-validation-for="PrCetificate" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="CategoryCertificate" class="control-label"></label>
                        <input type="file" asp-for="CategoryCertificate" class="form-control" />
                        <span asp-validation-for="CategoryCertificate" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="AgeProof" class="control-label"></label>
                        <input type="file" asp-for="AgeProof" class="form-control" />
                        <span asp-validation-for="AgeProof" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="Marksheet" class="control-label"></label>
                        <input type="file" asp-for="Marksheet" class="form-control" />
                        <span asp-validation-for="Marksheet" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="CharacterCertificate" class="control-label"></label>
                        <input type="file" asp-for="CharacterCertificate" class="form-control" />
                        <span asp-validation-for="CharacterCertificate" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <hr class="col-md-10" />
            <div class="row">
                <h3>Guardian Information</h3>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="GuardianName" class="control-label"></label>
                        <input asp-for="GuardianName" class="form-control" />
                        <span asp-validation-for="GuardianName" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="GuardianOccupation" class="control-label"></label>
                        <input asp-for="GuardianOccupation" class="form-control" />
                        <span asp-validation-for="GuardianOccupation" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="GuardianOfficeName" class="control-label"></label>
                        <input asp-for="GuardianOfficeName" class="form-control" />
                        <span asp-validation-for="GuardianOfficeName" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="GuardianOfficeAddress" class="control-label"></label>
                        <input asp-for="GuardianOfficeAddress" class="form-control" />
                        <span asp-validation-for="GuardianOfficeAddress" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <div class="form-group">
                        <label asp-for="GuardianEmployerCertificate" class="control-label"></label>
                        <input type="file" asp-for="GuardianEmployerCertificate" class="form-control" />
                        <span asp-validation-for="GuardianEmployerCertificate" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-5">
                </div>
            </div>


            <hr class="col-md-10" />
            <div class="row">
                <div class="form-group offset-4">
                  
                </div>
            </div>

        </form>
    </div>
</div>

<div>
    <a asp-action="Display">Back to List</a>
</div>


<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/cascade.js"></script>

